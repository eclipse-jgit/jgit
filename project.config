[project]
	description = Java implementation of Git (technology.jgit)
[receive]
	requireChangeId = true
[access]
	inheritFrom = permissions/jgit-dev
[plugin "uploadvalidator"]
	binaryTypes = image/png
	blockedContentTypeWhitelist = false
	maxPathLength = 0
	rejectDuplicatePathnames = true
	rejectDuplicatePathnamesLocale = en
	rejectSubmodule = true
	rejectSymlink = false
	rejectWindowsLineEndings = true
