[project]
	description = Java implementation of Git (technology.jgit)
[receive]
	requireChangeId = true
[access "refs/heads/*"]
	forgeAuthor = group ldap/technology.jgit
	label-Code-Review = -2..+2 group ldap/technology.jgit
	label-IP-Clean = -1..+1 group ldap/technology.jgit
	label-Verified = -1..+1 group ldap/technology.jgit
	submit = group ldap/technology.jgit
	abandon = group ldap/technology.jgit
	create = group ldap/technology.jgit
	pushMerge = group ldap/technology.jgit
[access "refs/tags/*"]
	pushTag = group ldap/technology.jgit
	create = group ldap/technology.jgit
	pushSignedTag = group ldap/technology.jgit
	push = group ldap/technology.jgit
[access "refs/*"]
	owner = group Administrators
